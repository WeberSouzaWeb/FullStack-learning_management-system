{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config":{
                "includeFiles": [
                    "dist/**","dist/**", "configs/**", "routes/**", "controllers/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        }
    ]
}